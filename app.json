{
  "expo": {
    "name": "Abbreviation Game",
    "slug": "abbreviation-game",
    "description": "A fun and educational quiz game where you guess the meaning of abbreviations.",
    "githubUrl": "https://github.com/NatasaIc/abbreviation-game-native",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.nestnnode.abbreviatios"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "package": "dev.nestnnode.abbreviations"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "plugins": [
      "expo-av"
    ],
    "extra": {
      "eas": {
        "projectId": "14d5251e-ca6e-445b-9144-0b6fa454e437"
      }
    }
  }
}
